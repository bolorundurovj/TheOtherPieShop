@model IEnumerable<TheOtherPieShop.Models.Pie>

<!doctype html>
<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <body>
        <h2>@ViewBag.Title</h2>

        @foreach(var pie in Model)
        {
            <div>
                <h2>@pie.Name</h2>
                <h3>@pie.Price.ToString("c")</h3>
            </div>
        }

    </body>
</head>
</html>

